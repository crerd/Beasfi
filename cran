// ==UserScript== // @name         beastfi // @namespace    http://tampermonkey.net/ // @version      0.1 // @description  Made by @mizqe // @author       @mizqe // @match        https://beastfi.org/deposits-rewards // @icon         https://www.google.com/s2/favicons?sz=64&domain=beastfi.org // @grant        none // ==/UserScript==  window.onload = (async function() {     'use strict';      console.log("Start!!!")     const delay = 120000      setTimeout(clickHarvest, 3000);      function clickHarvest() {         const harvestBtn = document.querySelector(".btn.btn--red.btn--big")         if(harvestBtn)             harvestBtn.click()         console.log("Harvest SUCCESS")         setTimeout(() => location.reload(), delay);     }  })();
